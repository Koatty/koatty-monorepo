{
  "name": "koatty_serve",
  "version": "3.1.5",
  "description": "Provide http1/2/3, websocket, gRPC server for Koatty.",
  "scripts": {
    "build": "npm run build:js && npm run build:dts && npm run build:doc && npm run build:cp",
    "build:cp": "node scripts/copyFiles && node scripts/postBuild",
    "build:js": "npx rollup --bundleConfigAsCjs -c .rollup.config.js",
    "build:doc": "npx api-documenter markdown --input temp --output docs/api",
    "build:dts": "bash ../../scripts/build-dts.sh",
    "eslint": "eslint --ext .ts,.js ./",
    "lock": "npx del-cli --force node_modules && npm i --package-lock-only && npx pnpm i",
    "test": "npm run eslint && jest --passWithNoTests",
    "test:cov": "jest --collectCoverage --detectOpenHandles"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koatty/koatty_serve.git"
  },
  "keywords": [
    "IOC",
    "koatty",
    "koatty_serve",
    "http",
    "https",
    "websocket",
    "grpc",
    "quic",
    "http2",
    "http3"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": {
    "name": "richenlin",
    "email": "richenlin@gmail.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/koatty/koatty_serve/issues"
  },
  "homepage": "https://github.com/koatty/koatty_serve",
  "maintainers": [
    {
      "name": "richenlin",
      "email": "richenlin@gmail.com"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.x.x",
    "@commitlint/config-conventional": "^19.x.x",
    "@grpc/grpc-js": "^1.x.x",
    "@microsoft/api-documenter": "^7.x.x",
    "@microsoft/api-extractor": "^7.x.x",
    "@rollup/plugin-commonjs": "^28.x.x",
    "@rollup/plugin-json": "^6.x.x",
    "@rollup/plugin-node-resolve": "^16.x.x",
    "@rollup/plugin-terser": "^0.x.x",
    "@types/jest": "^29.x.x",
    "@types/koa": "^2.x.x",
    "@types/koa-compose": "^3.x.x",
    "@types/lodash": "^4.x.x",
    "@types/node": "^22.x.x",
    "@types/on-finished": "^2.x.x",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.x.x",
    "@typescript-eslint/eslint-plugin": "^8.x.x",
    "@typescript-eslint/parser": "^8.x.x",
    "eslint": "^8.x.x",
    "eslint-plugin-jest": "^28.x.x",
    "husky": "^4.x.x",
    "jest": "^29.x.x",
    "jest-html-reporters": "^3.x.x",
    "reflect-metadata": "^0.x.x",
    "rollup": "^4.x.x",
    "rollup-plugin-delete": "^2.x.x",
    "rollup-plugin-typescript2": "^0.x.x",
    "supertest": "^7.1.0",
    "ts-jest": "^29.x.x",
    "ts-node": "^10.x.x",
    "tslib": "^2.x.x",
    "typescript": "^5.x.x"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@matrixai/quic": "^2.0.9",
    "koatty_container": "workspace:*",
    "koatty_core": "workspace:*",
    "koatty_exception": "workspace:*",
    "koatty_lib": "workspace:*",
    "koatty_logger": "workspace:*",
    "koatty_proto": "workspace:*",
    "koatty_validation": "workspace:*",
    "ws": "^8.19.0"
  }
}
