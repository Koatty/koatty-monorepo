{
  "name": "koatty_serve",
  "version": "3.1.7",
  "description": "Provide http1/2/3, websocket, gRPC server for Koatty.",
  "scripts": {
    "build": "pnpm run build:js && pnpm run build:dts && pnpm run build:doc && pnpm run build:cp",
    "build:js": "tsup",
    "build:dts": "bash ../../scripts/build-dts.sh",
    "build:doc": "api-documenter markdown --input temp --output docs/api",
    "build:cp": "node ../../scripts/postBuild.js",
    "clean": "rimraf dist temp docs/api",
    "lint": "eslint --ext .ts,.js ./src",
    "test": "pnpm run lint && jest --passWithNoTests",
    "test:cov": "jest --collectCoverage --detectOpenHandles"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koatty/koatty_serve.git"
  },
  "keywords": [
    "IOC",
    "koatty",
    "koatty_serve",
    "http",
    "https",
    "websocket",
    "grpc",
    "quic",
    "http2",
    "http3"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": {
    "name": "richenlin",
    "email": "richenlin@gmail.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/koatty/koatty_serve/issues"
  },
  "homepage": "https://github.com/koatty/koatty_serve",
  "maintainers": [
    {
      "name": "richenlin",
      "email": "richenlin@gmail.com"
    }
  ],
  "devDependencies": {
    "@grpc/grpc-js": "^1.x.x",
    "@microsoft/api-documenter": "^7.26.x",
    "@microsoft/api-extractor": "^7.52.x",
    "@types/jest": "^29.5.x",
    "@types/koa": "^2.x.x",
    "@types/koa-compose": "^3.x.x",
    "@types/lodash": "^4.x.x",
    "@types/node": "^22.x.x",
    "@types/on-finished": "^2.x.x",
    "@types/supertest": "^6.x.x",
    "@types/ws": "^8.x.x",
    "@typescript-eslint/eslint-plugin": "^8.x.x",
    "@typescript-eslint/parser": "^8.x.x",
    "eslint": "^8.x.x",
    "eslint-plugin-jest": "^28.x.x",
    "jest": "^29.x.x",
    "jest-html-reporters": "^3.x.x",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.x.x",
    "ts-jest": "^29.x.x",
    "tslib": "^2.x.x"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@matrixai/quic": "^2.0.9",
    "koatty_container": "workspace:*",
    "koatty_core": "workspace:*",
    "koatty_exception": "workspace:*",
    "koatty_lib": "workspace:*",
    "koatty_logger": "workspace:*",
    "koatty_proto": "workspace:*",
    "koatty_validation": "workspace:*",
    "ws": "^8.19.0"
  }
}
